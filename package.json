{
  "name": "fulltilt",
  "version": "1.0.0",
  "description": "FullTilt is a clean and fast blog theme for your Hugo site.",
  "main": "index.js",
  "repository": "https://github.com/apvarun/blist-hugo-theme",
  "author": "Steve Scargall",
  "license": "MIT",
  "scripts": {
    "start": "sudo hugo serve --bind 0.0.0.0 --port 80 --baseURL http://35.188.1.25 --verbose --theme fulltilt --templateMetrics --templateMetricsHints",
    "start-examplesite": "hugo server -s exampleSite --themesDir=../.. --disableFastRender",
    "build-examplesite": "NODE_ENV=production hugo -s exampleSite --themesDir=../.. --gc",
    "build": "NODE_ENV=production hugo --gc --minify"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.4.31",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.1",
    "tailwindcss": "^2.2.19"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
